        android:icon="@mipmap/ic_launcher_big"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.qflbai.love.LaunchActivity"
            android:theme="@style/LaunchActivityStyle"
            >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity android:name="com.qflbai.love.ShowActivity" />
        <activity android:name="com.qflbai.love.MainActivity">

        </activity>
    </application>

</manifest>